{
	"name": "smart-resource",
	"version": "2.0.0",
	"description": "An observable utility for asynchronous resources.",
	"keywords": [
		"observable",
		"resource",
		"fetch",
		"promise",
		"async"
	],
	"author": "erictooth",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/erictooth/smart-resource.git"
	},
	"homepage": "https://github.com/erictooth/smart-resource",
	"bugs": {
		"url": "https://github.com/erictooth/smart-resource/issues"
	},
	"files": [
		"dist-cjs",
		"dist-esm"
	],
	"main": "./dist-cjs/index.js",
