	"dependencies": {
		"rxjs": ">=5.0.0"
	},
	"devDependencies": {
		"@erictooth/eslint-config": "^8.0.0",
		"@swc/cli": "0.1.62",
		"@swc/core": "1.3.96",
		"@swc/jest": "0.2.29",
		"@types/jest": "^28",
		"@types/node": "^18.6.3",
		"eslint": "^8",
		"jest": "^28",
		"prettier": "3.0.3",
		"ts-node": "^10.9.1",
		"typescript": "5.2.2"
	},
	"packageManager": "pnpm@8.10.2"
